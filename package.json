{
  "name": "toite-instance-api",
  "version": "0.0.1",
  "description": "API for Toite Instance",
  "author": "Yefrosynii Kolenko <contact@yefro.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Toite-app/backend.git"
  },
  "homepage": "https://github.com/Toite-app/backend#readme",
  "bugs": {
    "url": "https://github.com/Toite-app/backend/issues"
  },
  "keywords": [
    "toite",
    "nestjs",
    "api",
    "backend",
    "restaurant",
    "management"
  ],
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:dev:second": "cross-env PORT=3001 nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --runInBand",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:migrate": "node -r esbuild-register src/@base/drizzle/migrate.ts",
    "db:migrate:drop": "drizzle-kit drop",
    "bench": "k6 run ./utils/bench/index.js"
  },
